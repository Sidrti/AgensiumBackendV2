          "default": 0.4,
          "min": 0,
          "max": 1,
          "show": false,
          "required": false
        },
        "pattern_match_weight": {
          "type": "float",
          "description": "Weight for pattern matching in scoring",
          "default": 0.3,
          "min": 0,
          "max": 1,
          "show": false,
          "required": false
        },
        "value_analysis_weight": {
          "type": "float",
          "description": "Weight for value analysis in scoring",
          "default": 0.3,
          "min": 0,
          "max": 1,
          "show": false,
          "required": false
        },
        "excellent_threshold": {
          "type": "integer",
          "description": "Score threshold for excellent quality status",
          "default": 90,
          "min": 0,
          "max": 100,
          "show": false,
          "required": false
        },
        "good_threshold": {
          "type": "integer",
          "description": "Score threshold for good quality status",
          "default": 75,
          "min": 0,
          "max": 100,
          "show": false,
          "required": false
        }
      }
    },
    "survivorship-resolver": {
      "id": "survivorship-resolver",
      "name": "Survivorship Resolver",
      "description": "Determines which field values should 'survive' when multiple sources provide different or conflicting values. Applies business rules, scoring logic, and priority models to pick the best possible value before the GoldenRecordBuilder constructs the final Golden Record.",
      "icon": "âš”ï¸",
      "category": "Conflict Resolution",
      "accuracy": "96%",
      "features": [
        "Conflict Detection",
        "Hierarchical Rule Engine",
        "Freshness/Timestamp Resolution",
        "Data Quality Scoring",
        "Value Length/Richness Analysis",
        "Source Priority Resolution",
        "Field-Level Custom Rules"
      ],
      "input": {
        "type": "CSV"
      },
      "version": "1.0.0",
      "required_files": ["primary"],
      "parameters": {
        "match_key_columns": {
          "type": "array",
          "description": "Columns to use for identifying related records",
          "default": [],
          "items": "string",
          "example": ["Email", "Phone"],
          "show": true,
          "required": true,
          "columnSelection": true
        },
        "survivorship_rules": {
          "type": "object",
          "description": "Column -> rule mapping (freshness, quality_score, completeness, source_priority, frequency, richness, validation, min, max, first, last)",
          "default": {},
          "example": {
            "email": "most_recent",
            "phone": "most_frequent",
            "address": "source_priority",
            "name": "most_complete",
            "score": "max"
          },
          "show": true,
          "required": false
        },
        "source_priority": {
          "type": "object",
          "description": "Source -> priority mapping (lower = higher priority)",
          "default": {},
          "example": {
            "CRM": 1,
            "ERP": 2,
            "Web": 3,
            "Legacy": 4
          },
          "show": true,
          "required": false
        },
        "source_column": {
          "type": "string",
          "description": "Column identifying the record source",
          "default": null,
          "example": "SourceSystem",
          "show": true,
          "required": true,
          "columnSelection": true
        },
        "timestamp_column": {
          "type": "string",
          "description": "Column for freshness-based resolution",
          "default": null,
          "example": "updated_at",
          "show": true,
          "required": false,
          "columnSelection": true
        },
        "default_rule": {
          "type": "string",
          "description": "Default rule when no specific rule is defined",
          "default": "quality_score",
          "allowed": [
            "freshness",
            "quality_score",
            "completeness",
            "source_priority",
            "frequency",
            "richness",
            "validation",
            "min",
            "max",
            "first",
            "last"
          ],
          "show": false,
          "required": false
        },
        "min_confidence_threshold": {
          "type": "float",
          "description": "Minimum confidence for considering a resolution successful",
          "default": 0.5,
          "min": 0,
          "max": 1,
          "show": false,
          "required": false
        },
        "field_validation_rules": {
          "type": "object",
          "description": "Field-specific validation rules (column -> {pattern, min_length, allowed_values})",
          "default": {},
          "example": {
            "email": { "pattern": "^.+@.+$" },
            "age": { "min": 18 }
          },
          "show": true,
          "required": false
        },
        "excellent_threshold": {
          "type": "integer",
          "description": "Score threshold for excellent quality status",
          "default": 90,
          "min": 0,
          "max": 100,
          "show": false,
          "required": false
        },
        "good_threshold": {
          "type": "integer",
          "description": "Score threshold for good quality status",
          "default": 75,
          "min": 0,
          "max": 100,
          "show": false,
          "required": false
        }
      }
    },
    "golden-record-builder": {
      "id": "golden-record-builder",
      "name": "Golden Record Builder",
      "description": "Creates the final unified, conflict-resolved, highest-quality version of a record (Golden Record). Merges records from multiple sources using survivorship rules to create a single source of truth.",
      "icon": "ðŸ†",
      "category": "Master Data",
      "accuracy": "97%",
      "features": [
        "Record Clustering",
        "Survivorship Rule Application",
        "Conflict Resolution",
        "Trust Score Calculation",
        "Source Attribution",
        "Compression Ratio Tracking"
      ],
      "input": {
        "type": "CSV"
      },
      "version": "1.0.0",
      "required_files": ["primary"],
      "parameters": {
        "match_key_columns": {
          "type": "array",
          "description": "Columns to use for identifying related records",
          "default": [],
          "items": "string",
          "example": ["Email", "Phone"],
          "required": true,
          "show": true,
          "columnSelection": true,
          "collectWeights": true,
          "weightsParamKey": "match_key_weights",
          "weightsRange": { "min": 0, "max": 100 }
