          "required": false
        },
        "good_threshold": {
          "type": "integer",
          "description": "Score threshold for good quality status",
          "default": 75,
          "min": 0,
          "max": 100,
          "show": false,
          "required": false
        }
      }
    },
    "stewardship-flagger": {
      "id": "stewardship-flagger",
      "name": "Stewardship Flagger",
      "description": "Identifies data issues that require human review or intervention. Automatically flags suspicious, incomplete, inconsistent, or low-confidence data so Data Stewards can take action. Acts as the quality gatekeeper of the entire data pipeline.",
      "icon": "ðŸš©",
      "category": "Governance",
      "accuracy": "95%",
      "features": [
        "Missing Required Field Detection",
        "Format Validation",
        "Low Confidence Detection",
        "Outlier Detection",
        "Duplicate Detection",
        "Business Rule Validation",
        "Stewardship Task Generation"
      ],
      "input": {
        "type": "CSV"
      },
      "version": "1.0.0",
      "required_files": ["primary"],
      "parameters": {
        "required_columns": {
