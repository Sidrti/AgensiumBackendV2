        },
        "source_column": {
          "type": "string",
          "description": "Column identifying the record source",
          "default": null,
          "example": "SourceSystem",
          "required": true,
          "show": true,
          "columnSelection": true
        },
        "timestamp_column": {
          "type": "string",
          "description": "Column for recency-based survivorship rules",
          "default": null,
          "example": "updated_at",
          "show": true,
          "required": false,
          "columnSelection": true
        },
        "default_survivorship_rule": {
          "type": "string",
          "description": "Default rule when no specific rule is defined",
          "default": "most_complete",
          "allowed": [
            "most_complete",
            "most_recent",
            "source_priority",
            "most_frequent",
            "min",
            "max",
            "first",
            "last"
          ],
          "show": false,
          "required": false
