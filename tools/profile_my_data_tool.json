{
  "tool": {
    "id": "profile-my-data",
    "name": "Profile My Data",
    "description": "Comprehensive data profiling and quality assessment tool. Analyze data completeness, quality metrics, detect patterns, identify risks, and assess readiness for analytics.",
    "icon": "ðŸ“Š",
    "category": "source",
    "isAvailable": true,
    "version": "2.0.0",
    "available_agents": [
      "unified-profiler",
      "drift-detector",
      "score-risk",
      "governance-checker",
      "test-coverage-agent",
      "readiness-rater"
    ],
    "files": {
      "primary": {
        "description": "Current data file to profile",
        "required": true,
        "formats": ["csv", "json", "xlsx"],
        "max_size_mb": 500
      },
      "baseline": {
        "description": "Baseline data file for drift detection (optional, required only if drift-detector is selected)",
        "required": false,
        "formats": ["csv", "json", "xlsx"],
        "max_size_mb": 500
      }
    }
  },
  "agents": {
    "unified-profiler": {
      "id": "unified-profiler",
      "name": "Unified Profiler",
      "description": "Comprehensive data profiling with statistics and quality metrics. Analyzes data completeness, validity, consistency, and identifies quality issues across all fields.",
      "icon": "ðŸ“ˆ",
      "category": "Data Analysis",
      "accuracy": "99%",
      "features": [
        "Statistical Analysis",
        "Quality Scoring",
        "Outlier Detection",
        "Distribution Analysis"
      ],
      "input": {
        "type": "CSV, JSON, XLSX"
      },
      "version": "1.0.0",
      "required_files": ["primary"],
      "parameters": {
        "null_alert_threshold": {
          "type": "integer",
          "description": "Threshold for null value alerts (%)",
          "default": 50,
          "min": 0,
          "max": 100,
          "show": false,
          "required": false
        },
        "categorical_threshold": {
          "type": "integer",
          "description": "Threshold for categorical field detection",
          "default": 20,
          "min": 1,
          "show": false,
          "required": false
        },
        "categorical_ratio_threshold": {
          "type": "float",
          "description": "Ratio threshold for categorical analysis",
          "default": 0.05,
          "min": 0,
          "max": 1,
          "show": false,
          "required": false
        },
        "top_n_values": {
          "type": "integer",
          "description": "Number of top values to track",
          "default": 10,
          "min": 1,
          "show": false,
          "required": false
        },
        "outlier_iqr_multiplier": {
          "type": "float",
          "description": "IQR multiplier for outlier detection",
          "default": 1.5,
          "min": 0.5,
          "show": false,
          "required": false
        },
        "outlier_alert_threshold": {
          "type": "integer",
          "description": "Threshold for outlier alerts (%)",
          "default": 5,
          "min": 0,
          "max": 100,
          "show": false,
          "required": false
        }
      }
    },
    "drift-detector": {
      "id": "drift-detector",
      "name": "Drift Detector",
      "description": "Detects changes (drift) between baseline and current datasets. Identifies statistical shifts, distribution changes, and anomalies using multiple statistical tests.",
      "icon": "ï¿½",
      "category": "Comparison",
      "accuracy": "97%",
      "features": [
        "Statistical Testing",
        "Distribution Comparison",
        "PSI Scoring",
        "Change Detection"
      ],
      "input": {
        "type": "CSV, JSON, XLSX"
      },
      "version": "1.0.0",
      "required_files": ["primary", "baseline"],
      "parameters": {
        "statistical_test": {
          "type": "string",
          "description": "Statistical test method",
          "default": "kolmogorov_smirnov",
          "allowed": ["kolmogorov_smirnov", "chi2", "wasserstein"],
          "show": false,
          "required": false
        },
        "significance_level": {
          "type": "float",
          "description": "Significance level for statistical tests",
          "default": 0.05,
          "min": 0,
          "max": 1,
          "show": false,
          "required": false
        },
        "min_sample_size": {
          "type": "integer",
          "description": "Minimum sample size for analysis",
          "default": 100,
          "min": 10,
          "show": false,
          "required": false
        }
      }
    },
    "score-risk": {
      "id": "score-risk",
      "name": "Risk Scorer",
      "description": "Identifies PII, calculates risk scores, and assesses compliance requirements. Detects sensitive data, evaluates governance gaps, and provides security assessments.",
      "icon": "ðŸ›¡ï¸",
      "category": "Security",
      "accuracy": "96%",
      "features": [
        "PII Detection",
        "Risk Scoring",
        "Compliance Check",
        "Governance Gap Analysis"
      ],
      "input": {
        "type": "CSV, JSON, XLSX"
      },
      "version": "1.0.0",
      "required_files": ["primary"],
      "parameters": {
        "pii_sample_size": {
          "type": "integer",
          "description": "Sample size for PII detection",
          "default": 100,
          "min": 10,
          "show": false,
          "required": false
        },
        "high_risk_threshold": {
          "type": "integer",
          "description": "Score threshold for high risk classification",
          "default": 70,
          "min": 0,
          "max": 100,
          "show": false,
          "required": false
        },
        "medium_risk_threshold": {
          "type": "integer",
          "description": "Score threshold for medium risk classification",
          "default": 40,
          "min": 0,
          "max": 100,
          "show": false,
          "required": false
        },
        "pii_detection_enabled": {
          "type": "boolean",
          "description": "Enable PII detection",
          "default": true,
          "show": false,
          "required": false
        },
        "sensitive_field_detection_enabled": {
          "type": "boolean",
          "description": "Enable sensitive field detection",
          "default": true,
          "show": false,
          "required": false
        },
        "governance_check_enabled": {
          "type": "boolean",
          "description": "Enable governance checks",
          "default": true,
          "show": false,
          "required": false
        }
      }
    },
    "test-coverage-agent": {
      "id": "test-coverage-agent",
      "name": "Test Coverage Agent",
      "description": "Validates data test coverage including uniqueness, range, and format constraints. Ensures data meets quality and validation requirements for testing.",
      "icon": "ðŸ§ª",
      "category": "Testing",
      "accuracy": "97%",
      "features": [
        "Uniqueness Testing",
        "Range Validation",
        "Format Validation",
        "Coverage Scoring"
      ],
      "input": {
        "type": "CSV, JSON, XLSX"
      },
      "version": "1.0.0",
      "required_files": ["primary"],
      "parameters": {
        "uniqueness_weight": {
          "type": "float",
          "description": "Weight for uniqueness tests",
          "default": 0.4,
          "min": 0,
          "max": 1,
          "show": false,
          "required": false
        },
        "range_weight": {
          "type": "float",
          "description": "Weight for range tests",
          "default": 0.3,
          "min": 0,
          "max": 1,
          "show": false,
          "required": false
        },
        "format_weight": {
          "type": "float",
          "description": "Weight for format tests",
          "default": 0.3,
          "min": 0,
          "max": 1,
          "show": false,
          "required": false
        },
        "excellent_threshold": {
          "type": "integer",
          "description": "Score threshold for excellent status",
          "default": 90,
          "min": 0,
          "max": 100,
          "show": false,
          "required": false
        },
        "good_threshold": {
          "type": "integer",
          "description": "Score threshold for good status",
          "default": 75,
          "min": 0,
          "max": 100,
          "show": false,
          "required": false
        },
        "unique_columns": {
          "type": "array",
          "description": "List of columns that should have unique values",
          "default": [],
          "items": "string",
          "example": ["user_id", "email", "ssn", "order_id"],
          "show": true,
          "required": false
        },
        "range_tests": {
          "type": "object",
          "description": "Range constraints for numeric columns (column -> {min, max})",
          "default": {},
          "example": {
            "age": { "min": 0, "max": 120 },
            "rating": { "min": 1, "max": 5 },
            "percentage": { "min": 0, "max": 100 }
          },
          "show": true,
          "required": false
        },
        "format_tests": {
          "type": "object",
          "description": "Format constraints for columns (column -> pattern or {pattern, description})",
          "default": {},
          "example": {
            "email": {
              "pattern": "^[^@]+@[^@]+\\.[^@]+$",
              "description": "Valid email address"
            },
            "sku": "^[A-Z]{3}-\\d{4}$",
            "phone": "^\\+?[1-9]\\d{1,14}$"
          },
          "show": true,
          "required": false
        }
      }
    },
    "readiness-rater": {
      "id": "readiness-rater",
      "name": "Readiness Rater",
      "description": "Rates data readiness for analysis based on quality metrics. Evaluates completeness, consistency, and schema health to determine if data is production-ready.",
      "icon": "ðŸš¦",
