{
  "tool": {
    "id": "profile-my-data",
    "name": "Profile My Data",
    "description": "Comprehensive data profiling and quality assessment tool. Analyze data completeness, quality metrics, detect patterns, identify risks, and assess readiness for analytics.",
    "icon": "ğŸ“Š",
    "category": "source",
    "isAvailable": true,
    "version": "2.0.0",
    "available_agents": [
      "unified-profiler",
      "drift-detector",
      "score-risk",
      "governance-checker",
      "test-coverage-agent",
      "readiness-rater"
    ],
    "files": {
      "primary": {
        "description": "Current data file to profile",
        "required": true,
        "formats": ["csv", "json", "xlsx"],
        "max_size_mb": 500
      },
      "baseline": {
        "description": "Baseline data file for drift detection (optional, required only if drift-detector is selected)",
        "required": false,
        "formats": ["csv", "json", "xlsx"],
        "max_size_mb": 500
      }
    }
  },
  "agents": {
    "unified-profiler": {
      "id": "unified-profiler",
      "name": "Unified Profiler",
      "description": "Comprehensive data profiling with statistics and quality metrics. Analyzes data completeness, validity, consistency, and identifies quality issues across all fields.",
      "icon": "ğŸ“ˆ",
      "category": "Data Analysis",
      "accuracy": "99%",
      "features": [
        "Statistical Analysis",
        "Quality Scoring",
        "Outlier Detection",
        "Distribution Analysis"
      ],
      "input": { "type": "CSV, JSON, XLSX" },
      "version": "1.0.0",
      "required_files": ["primary"],
      "parameters": {
        "null_alert_threshold": {
          "type": "integer",
          "description": "Threshold for null value alerts (%)",
          "default": 50,
          "min": 0,
          "max": 100
        },
        "categorical_threshold": {
          "type": "integer",
          "description": "Threshold for categorical field detection",
          "default": 20,
          "min": 1
        },
        "categorical_ratio_threshold": {
          "type": "float",
          "description": "Ratio threshold for categorical analysis",
          "default": 0.05,
          "min": 0,
          "max": 1
        },
        "top_n_values": {
          "type": "integer",
          "description": "Number of top values to track",
          "default": 10,
          "min": 1
        },
        "outlier_iqr_multiplier": {
          "type": "float",
          "description": "IQR multiplier for outlier detection",
          "default": 1.5,
          "min": 0.5
        },
        "outlier_alert_threshold": {
          "type": "integer",
          "description": "Threshold for outlier alerts (%)",
          "default": 5,
          "min": 0,
          "max": 100
        }
      },
      "output_structure": {
        "agent_id": "string",
        "agent_name": "string",
        "status": "string",
        "execution_time_ms": "integer",
        "summary_metrics": {
          "total_columns": "integer",
          "total_rows": "integer",
          "columns_with_issues": "integer"
        },
        "data": {
          "fields": [
            {
              "field_id": "string",
              "field_name": "string",
              "data_type": "string",
              "semantic_type": "string",
              "properties": "object",
              "statistics": "object",
              "distribution": "object",
              "quality_score": "number",
              "quality_indicators": "object"
            }
          ],
          "quality_summary": "object"
        }
      }
    },
    "drift-detector": {
      "id": "drift-detector",
      "name": "Drift Detector",
      "description": "Detects changes (drift) between baseline and current datasets. Identifies statistical shifts, distribution changes, and anomalies using multiple statistical tests.",
      "icon": "ï¿½",
      "category": "Comparison",
      "accuracy": "97%",
      "features": [
        "Statistical Testing",
        "Distribution Comparison",
        "PSI Scoring",
        "Change Detection"
      ],
      "input": { "type": "CSV, JSON, XLSX" },
      "version": "1.0.0",
      "required_files": ["primary", "baseline"],
      "parameters": {
        "statistical_test": {
          "type": "string",
          "description": "Statistical test method",
          "default": "kolmogorov_smirnov",
          "allowed": ["kolmogorov_smirnov", "chi2", "wasserstein"]
        },
        "significance_level": {
          "type": "float",
          "description": "Significance level for statistical tests",
          "default": 0.05,
          "min": 0,
          "max": 1
        },
        "min_sample_size": {
          "type": "integer",
          "description": "Minimum sample size for analysis",
          "default": 100,
          "min": 10
        }
      },
      "output_structure": {
        "agent_id": "string",
        "agent_name": "string",
        "status": "string",
        "execution_time_ms": "integer",
        "summary_metrics": {
          "fields_analyzed": "integer",
          "drift_detected_count": "integer",
          "drift_severity_high": "integer",
          "drift_severity_medium": "integer",
          "drift_severity_low": "integer",
          "average_psi_score": "number"
        },
        "data": {
          "fields": [
            {
              "field_id": "string",
              "field_name": "string",
              "baseline_statistics": "object",
              "current_statistics": "object",
              "drift_analysis": "object",
              "drift_interpretation": "object",
              "issue_severity": "string"
            }
          ],
          "drift_summary": "object"
        }
      }
    },
    "score-risk": {
