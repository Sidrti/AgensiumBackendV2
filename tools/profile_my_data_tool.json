  "response_structure": {
    "status": "string",
    "timestamp": "string (ISO 8601)",
    "execution_time_ms": "integer",
    "report": {
      "common_arrays": {
        "alerts": "array - Contains critical alerts across all selected agents",
        "routing": "array - Routing information for different systems",
        "issues": "array - All issues detected by selected agents",
        "recommendations": "array - Recommendations from all selected agents",
        "executiveSummary": "array - High-level summary metrics",
        "visualizations": "array - NEW: Graph-ready data structures for direct frontend use (see visualizations section below)"
      },
      "agent_results": "object - Individual agent results keyed by agent_id (unified-profiler, drift-detector, score-risk, readiness-rater)",
      "downloads": "array - Available downloadable reports"
    }
  },
