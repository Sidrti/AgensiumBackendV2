        "Health Status"
      ],
      "input": {
        "type": "CSV, JSON, XLSX"
      },
      "version": "1.0.0",
      "required_files": ["primary"],
      "parameters": {
        "ready_threshold": {
          "type": "integer",
          "description": "Score threshold for 'READY' status (0-100)",
          "default": 80,
          "min": 0,
          "max": 100,
          "show": false,
          "required": false
        },
        "needs_review_threshold": {
          "type": "integer",
          "description": "Score threshold for 'NEEDS_REVIEW' status (0-100)",
          "default": 50,
          "min": 0,
          "max": 100,
          "show": false,
          "required": false
        },
        "completeness_weight": {
          "type": "float",
          "description": "Weight for completeness component",
          "default": 0.3,
          "min": 0,
          "max": 1,
          "show": false,
          "required": false
        },
        "consistency_weight": {
          "type": "float",
          "description": "Weight for consistency component",
          "default": 0.3,
          "min": 0,
          "max": 1,
          "show": false,
          "required": false
        },
        "schema_health_weight": {
          "type": "float",
          "description": "Weight for schema health component",
          "default": 0.4,
          "min": 0,
          "max": 1,
          "show": false,
          "required": false
        }
      }
    }
  }
}
