{
  "tool": {
    "id": "master-my-data",
    "name": "Master My Data",
    "description": "Comprehensive Master Data Management (MDM) tool. Identify keys, enforce contracts, map semantics, trace lineage, build golden records, resolve survivorship, and manage stewardship tasks to ensure a single source of truth.",
    "icon": "ðŸ‘‘",
    "category": "master",
    "isAvailable": true,
    "version": "1.0.0",
    "available_agents": [
      "key-identifier",
      "contract-enforcer",
      "semantic-mapper",
      "lineage-tracer",
      "golden-record-builder",
      "survivorship-resolver",
      "master-writeback-agent",
      "stewardship-flagger"
    ],
    "files": {
      "primary": {
        "description": "Data file to master",
        "required": true,
        "formats": ["csv", "json", "xlsx"],
        "max_size_mb": 1000
      },
      "schema": {
        "description": "Data contract or schema definition (optional)",
        "required": false,
        "formats": ["json", "yaml"],
        "max_size_mb": 10
      }
    }
  },
  "agents": {
    "key-identifier": {
      "id": "key-identifier",
      "name": "Key Identifier",
      "description": "Analyzes data to identify primary keys, composite keys, and candidate keys. Validates uniqueness constraints and structural integrity of identifiers.",
      "icon": "ðŸ”‘",
      "category": "Data Structure",
      "accuracy": "99%",
      "features": [
        "Primary Key Detection",
        "Composite Key Analysis",
        "Candidate Key Identification",
        "Uniqueness Validation",
        "Key Integrity Check"
      ],
      "input": { "type": "CSV, JSON, XLSX" },
      "version": "1.0.0",
      "required_files": ["primary"],
      "parameters": {
        "detect_composite_keys": {
          "type": "boolean",
          "description": "Attempt to identify composite keys from multiple columns",
          "default": true
        },
        "uniqueness_threshold": {
          "type": "float",
          "description": "Percentage of unique values required to consider a column a potential key",
          "default": 99.9,
          "min": 0,
          "max": 100
        },
        "max_composite_columns": {
          "type": "integer",
          "description": "Maximum number of columns to consider for a composite key",
          "default": 3,
          "min": 2,
          "max": 10
        },
        "ignore_nulls": {
          "type": "boolean",
          "description": "Ignore null values when checking for uniqueness",
          "default": false
        }
      },
      "output_structure": {
        "agent_id": "string",
        "agent_name": "string",
        "status": "string",
        "execution_time_ms": "integer",
        "summary_metrics": {
          "columns_analyzed": "integer",
          "keys_identified": "integer",
          "composite_keys_found": "integer"
        },
        "data": {
          "identified_keys": "array",
          "key_candidates": "array",
          "uniqueness_violations": "array"
        }
      }
    },
    "contract-enforcer": {
      "id": "contract-enforcer",
      "name": "Contract Enforcer",
      "description": "Validates data against defined data contracts and schemas. Ensures adherence to type definitions, constraints, allowed values, and structural requirements.",
      "icon": "ðŸ“œ",
      "category": "Governance",
      "accuracy": "98%",
      "features": [
        "Schema Validation",
        "Constraint Enforcement",
        "Data Type Verification",
        "Enum/Allowed Value Check",
        "Contract Compliance Scoring"
      ],
      "input": { "type": "CSV, JSON, XLSX + Schema" },
      "version": "1.0.0",
      "required_files": ["primary"],
      "parameters": {
        "strict_mode": {
          "type": "boolean",
          "description": "Fail validation on any contract violation",
          "default": false
        },
        "allow_extra_columns": {
          "type": "boolean",
          "description": "Allow columns not defined in the contract",
          "default": true
        },
        "validate_types": {
          "type": "boolean",
          "description": "Enforce strict data type validation",
          "default": true
        },
        "validate_constraints": {
          "type": "boolean",
          "description": "Enforce value constraints (min, max, regex)",
          "default": true
        }
      },
      "output_structure": {
        "agent_id": "string",
        "agent_name": "string",
        "status": "string",
        "execution_time_ms": "integer",
        "summary_metrics": {
          "total_records": "integer",
          "compliant_records": "integer",
          "non_compliant_records": "integer",
          "compliance_score": "number"
        },
        "data": {
          "violations": "array",
          "schema_diff": "object",
          "field_compliance": "object"
        }
      }
    },
    "semantic-mapper": {
      "id": "semantic-mapper",
      "name": "Semantic Mapper",
      "description": "Maps physical data fields to business glossary terms and semantic concepts. Uses NLP and pattern matching to infer meaning and standardize terminology.",
      "icon": "ðŸ§ ",
      "category": "Metadata Management",
      "accuracy": "95%",
      "features": [
        "Business Term Mapping",
        "Semantic Type Inference",
        "Ontology Alignment",
        "Metadata Enrichment"
      ],
      "input": { "type": "CSV, JSON, XLSX" },
      "version": "1.0.0",
      "required_files": ["primary"],
      "parameters": {
        "confidence_threshold": {
          "type": "float",
          "description": "Minimum confidence score to automatically accept a mapping",
          "default": 0.8,
          "min": 0,
          "max": 1
        },
        "auto_map": {
          "type": "boolean",
          "description": "Automatically apply high-confidence mappings",
          "default": true
        },
        "domain_context": {
          "type": "string",
          "description": "Business domain context to aid mapping (e.g., 'Finance', 'Healthcare')",
          "default": "General"
        }
      },
      "output_structure": {
        "agent_id": "string",
        "agent_name": "string",
        "status": "string",
        "execution_time_ms": "integer",
        "summary_metrics": {
          "fields_mapped": "integer",
          "pending_review": "integer",
          "average_confidence": "number"
        },
        "data": {
          "mappings": "array",
          "suggested_terms": "array",
          "semantic_types": "object"
        }
      }
    },
    "lineage-tracer": {
      "id": "lineage-tracer",
      "name": "Lineage Tracer",
      "description": "Tracks the origin and transformation history of data elements. Visualizes data flow and dependencies to ensure transparency and auditability.",
      "icon": "ðŸ•¸ï¸",
      "category": "Governance",
      "accuracy": "99%",
      "features": [
        "Source Tracking",
        "Transformation History",
        "Dependency Mapping",
        "Impact Analysis"
      ],
      "input": { "type": "CSV, JSON, XLSX" },
      "version": "1.0.0",
      "required_files": ["primary"],
      "parameters": {
        "trace_depth": {
          "type": "integer",
          "description": "Number of levels to trace back in lineage",
          "default": 5,
          "min": 1
        },
        "include_transformations": {
          "type": "boolean",
          "description": "Include details of transformations applied at each step",
          "default": true
        },
        "granularity": {
          "type": "string",
          "description": "Level of lineage detail",
          "default": "column",
          "allowed": ["table", "column", "row"]
        }
      },
      "output_structure": {
        "agent_id": "string",
        "agent_name": "string",
        "status": "string",
        "execution_time_ms": "integer",
        "summary_metrics": {
          "sources_identified": "integer",
          "transformations_tracked": "integer",
          "max_depth": "integer"
        },
        "data": {
          "lineage_graph": "object",
          "source_map": "object",
          "transformation_log": "array"
        }
      }
    },
    "golden-record-builder": {
      "id": "golden-record-builder",
      "name": "Golden Record Builder",
      "description": "Creates a single, authoritative 'Golden Record' for each entity by clustering and merging duplicate or related records across sources.",
      "icon": "ðŸ†",
      "category": "Master Data",
      "accuracy": "97%",
      "features": [
        "Entity Resolution",
        "Record Clustering",
        "Fuzzy Matching",
        "Golden Record Construction"
      ],
      "input": { "type": "CSV, JSON, XLSX" },
      "version": "1.0.0",
      "required_files": ["primary"],
      "parameters": {
        "match_columns": {
          "type": "array",
          "description": "Columns to use for matching records",
          "default": [],
          "items": "string"
        },
        "similarity_threshold": {
          "type": "float",
          "description": "Threshold for fuzzy matching similarity",
          "default": 0.85,
          "min": 0,
          "max": 1
        },
        "blocking_keys": {
          "type": "array",
          "description": "Columns to use for blocking (pre-filtering matches)",
          "default": [],
          "items": "string"
        },
        "clustering_algorithm": {
          "type": "string",
          "description": "Algorithm for grouping records",
          "default": "connected_components",
          "allowed": [
            "connected_components",
            "center_clustering",
            "merge_center"
          ]
        }
      },
      "output_structure": {
        "agent_id": "string",
        "agent_name": "string",
        "status": "string",
        "execution_time_ms": "integer",
        "summary_metrics": {
          "input_records": "integer",
          "golden_records_created": "integer",
          "clusters_formed": "integer",
          "compression_ratio": "number"
        },
        "data": {
          "clusters": "array",
          "match_statistics": "object",
          "golden_records_preview": "array"
        }
      }
    },
    "survivorship-resolver": {
      "id": "survivorship-resolver",
      "name": "Survivorship Resolver",
      "description": "Determines the best value for each attribute in a golden record based on configurable survivorship rules (e.g., recency, source priority, frequency).",
      "icon": "ðŸ›¡ï¸",
      "category": "Master Data",
      "accuracy": "98%",
      "features": [
        "Rule-Based Selection",
        "Source Prioritization",
        "Recency Handling",
        "Frequency Analysis",
        "Manual Override Support"
      ],
      "input": { "type": "Clustered Data" },
      "version": "1.0.0",
      "required_files": ["primary"],
      "parameters": {
        "default_strategy": {
          "type": "string",
          "description": "Default survivorship strategy for all fields",
          "default": "most_recent",
          "allowed": [
            "most_recent",
            "most_frequent",
            "source_priority",
            "completeness",
            "aggregation"
          ]
        },
        "field_strategies": {
          "type": "object",
          "description": "Specific strategies per field (field_name -> strategy)",
          "default": {}
        },
        "source_priority": {
          "type": "array",
          "description": "Ordered list of sources by priority",
          "default": [],
          "items": "string"
        },
        "recency_column": {
          "type": "string",
          "description": "Column indicating record timestamp for recency rules",
          "default": "updated_at"
        }
      },
      "output_structure": {
        "agent_id": "string",
        "agent_name": "string",
        "status": "string",
        "execution_time_ms": "integer",
        "summary_metrics": {
          "conflicts_resolved": "integer",
          "values_survived": "integer",
          "rules_applied": "integer"
        },
        "data": {
          "resolution_log": "array",
          "survivorship_stats": "object"
        }
      }
    },
    "master-writeback-agent": {
      "id": "master-writeback-agent",
      "name": "Master Writeback Agent",
      "description": "Commits the finalized master data to the target system or file. Ensures data integrity, generates audit logs, and handles versioning of master records.",
      "icon": "ðŸ’¾",
      "category": "Data Integration",
      "accuracy": "100%",
      "features": [
        "Data Export",
        "Audit Logging",
        "Version Control",
        "Target Validation",
        "Change Data Capture"
      ],
      "input": { "type": "Golden Records" },
      "version": "1.0.0",
      "required_files": ["primary"],
      "parameters": {
        "target_format": {
          "type": "string",
          "description": "Format for the output file",
          "default": "csv",
          "allowed": ["csv", "json", "parquet", "database"]
        },
        "include_audit_log": {
          "type": "boolean",
          "description": "Generate a separate audit log file",
          "default": true
        },
        "versioning_enabled": {
          "type": "boolean",
          "description": "Maintain version history for records",
          "default": true
        },
        "overwrite_mode": {
          "type": "string",
          "description": "Strategy for existing data",
          "default": "append",
          "allowed": ["overwrite", "append", "merge"]
        }
      },
      "output_structure": {
        "agent_id": "string",
        "agent_name": "string",
        "status": "string",
        "execution_time_ms": "integer",
        "summary_metrics": {
          "records_written": "integer",
          "bytes_written": "integer",
          "versions_created": "integer"
        },
        "data": {
          "output_location": "string",
          "audit_log_location": "string",
          "write_stats": "object"
        }
      }
    },
    "stewardship-flagger": {
      "id": "stewardship-flagger",
      "name": "Stewardship Flagger",
      "description": "Identifies ambiguous matches, low-confidence resolutions, and data quality issues that require human intervention. Creates tasks for data stewards.",
      "icon": "ðŸš©",
      "category": "Workflow",
      "accuracy": "96%",
      "features": [
        "Anomaly Detection",
        "Ambiguity Flagging",
        "Task Creation",
        "Priority Assignment",
        "Review Queue Management"
      ],
      "input": { "type": "Processed Data" },
      "version": "1.0.0",
      "required_files": ["primary"],
      "parameters": {
        "review_threshold": {
          "type": "float",
          "description": "Confidence score below which a record is flagged for review",
          "default": 0.7,
          "min": 0,
          "max": 1
        },
        "flag_ambiguous_matches": {
          "type": "boolean",
          "description": "Flag records with multiple potential match candidates",
          "default": true
        },
        "priority_level": {
          "type": "string",
          "description": "Default priority for created tasks",
          "default": "medium",
          "allowed": ["low", "medium", "high", "critical"]
        },
        "max_tasks_per_run": {
          "type": "integer",
          "description": "Limit the number of tasks generated",
          "default": 1000
        }
      },
      "output_structure": {
        "agent_id": "string",
        "agent_name": "string",
        "status": "string",
        "execution_time_ms": "integer",
        "summary_metrics": {
          "tasks_created": "integer",
          "high_priority_tasks": "integer",
          "records_flagged": "integer"
        },
        "data": {
          "task_list": "array",
          "flagged_records": "array",
          "reason_distribution": "object"
        }
      }
    }
  },
  "response_structure": {
    "status": "string",
    "timestamp": "string (ISO 8601)",
    "execution_time_ms": "integer",
    "report": {
      "common_arrays": {
        "alerts": "array - Critical alerts regarding master data integrity",
        "issues": "array - Detailed issues requiring attention",
        "recommendations": "array - Suggestions for improving data mastery",
        "executiveSummary": "array - High-level MDM metrics",
        "visualizations": "array - Graph-ready data structures"
      },
      "agent_results": "object - Individual agent results keyed by agent_id",
      "downloads": "array - Available downloadable reports (Golden Records, Audit Logs)"
    }
  },
